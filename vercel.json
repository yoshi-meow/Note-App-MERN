{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "server/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/notes",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      },
      {
        "src": "/(.*)"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb-uri",
      "API_KEY": "notes"
    }
  }
  
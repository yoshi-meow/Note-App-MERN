{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb-uri",
      "API_KEY": "my-secret-api-key"
    }
}
  